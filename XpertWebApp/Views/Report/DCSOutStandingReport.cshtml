        var rbtOnlyReduceDeduction = $("#rbtOnlyReduceDeductionCurrentDeduction").is(":checked");
        var rbtActive = $("#rbtActive").is(":checked");
        var rbtInActive = $("#rbtInActive").is(":checked");
        var rbtAll = $("#rbtAll").is(":checked");
        $.ajax({
            url: '/Report/ExportGetDCSOutStandingReport',
            type: 'GET',
            data: {
                fromDate: fromDate,
                toDate: toDate,
                mccCode: mccCode,
                deductionCode: deductionCode,
                chkDCSWise: chkDCSWise,
                rbtOldOutStanding: rbtOldOutStanding,
                rbtCurrentOpeningDeduction: rbtCurrentOpeningDeduction,
                rbtCurrentOutStanding: rbtCurrentOutStanding,
                rbtOnlyOpening: rbtOnlyOpening,
                rbtOnlyReduceDeduction: rbtOnlyReduceDeduction,
                rbtActive: rbtActive,
                rbtInActive: rbtInActive,
                rbtAll: rbtAll,
                btnPrint: false,
                AreaWiseBilling: false,
                fndArea: "",
                 pdf: pdf, excel: excel
            },

            xhrFields: {
                responseType: 'blob' // Ensure response is treated as a Blob
            },
            success: function (response) {
                if (excel == true) {
                    var url = URL.createObjectURL(response);
                    var link = document.createElement('a');
                    link.href = url;
                    link.download = 'Daily Qty Report.xlsx';
                    document.body.appendChild(link);
                    link.click();
                    document.body.removeChild(link);
                    URL.revokeObjectURL(url);
                }
                else if (pdf == true) {
                    var blob = new Blob([response], { type: 'application/pdf' });
                    var blobUrl = window.URL.createObjectURL(blob);
                    window.open(blobUrl, '_blank');
                }

            },
            error: function (xhr, status, error) {
                alert("An error occurred while generating the Excel file: " + error);
            }
        })
    }
</script>

