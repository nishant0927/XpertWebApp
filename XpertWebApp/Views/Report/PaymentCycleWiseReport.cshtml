        var modalId = "modalCreate";
        var title = "Select MCC / BMC";
        var header = ['', 'CODE', 'Name'];
        $("#modalCreate").data('source', 'mcc');
        GetMCC(function (data) {
            var filteredData = data.map(function (row) {
                return {
                    'CODE': row.Code,
                    'Name': row.Name
                }
            });
            AddMasterData(modalId, header, title, filteredData);
            disable();
        })
    })
    $("#btnloadRout").on('click', function () {
