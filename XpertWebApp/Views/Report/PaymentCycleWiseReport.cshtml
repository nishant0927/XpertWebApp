        $.ajax({
            url: '/Common/GetMcc',
            type: 'POST',
            dataType: "json",
            async: false,
            success: function (reponse) {
                if (reponse.lst) {
                    var responseData = JSON.parse(reponse.lst);
                    callback(responseData);
                }
                else {
                    ShowSweet2Message("warning", "Warning", reponse.responseText);
